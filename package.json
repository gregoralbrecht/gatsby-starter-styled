{
  "name": "gatsby-starter-styled",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and styled-system",
  "version": "2.0.0",
  "author": "Gregor Albrecht <hello@gregoralbrecht.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled"
  },
  "bugs": {
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled/issues"
  },
  "keywords": [
    "react",
    "gatsby",
    "styled-components",
    "styled-system"
  ],
  "scripts": {
    "start": "gatsby develop -o",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{jsx,js}\"",
    "lint:scripts": "eslint './src/**/*.{jsx,js}'",
    "lint:styles": "stylelint './src/**/*.{jsx,js}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint:styles",
      "pre-push": "yarn lint:scripts"
    }
  },
  "dependencies": {
    "@styled-system/prop-types": "^5.1.4",
    "@styled-system/theme-get": "^5.1.2",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.18.22",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-catch-links": "^2.1.24",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-lodash": "^3.1.19",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-plugin-typography": "^2.3.20",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "styled-components": "^4.4.1",
    "styled-system": "^5.1.4",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^9.4.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.26",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0"
  }
}
