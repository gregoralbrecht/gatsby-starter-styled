{
  "name": "gatsby-starter-styled",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and styled-system",
  "version": "2.0.0",
  "author": "Gregor Albrecht <hello@gregoralbrecht.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled"
  },
  "bugs": {
    "url": "https://github.com/gregoralbrecht/gatsby-starter-styled/issues"
  },
  "keywords": [
    "react",
    "gatsby",
    "styled-components",
    "styled-system"
  ],
  "scripts": {
    "start": "gatsby develop -o",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{jsx,js}\"",
    "lint:scripts": "eslint './src/**/*.{jsx,js}'",
    "lint:styles": "stylelint './src/**/*.{jsx,js}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint:styles",
      "pre-push": "yarn lint:scripts"
    }
  },
  "dependencies": {
    "@styled-system/prop-types": "^5.1.2",
    "@styled-system/theme-get": "^5.1.2",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.16.5",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-google-analytics": "^2.1.21",
    "gatsby-plugin-lodash": "^3.1.15",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-offline": "^3.0.16",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-plugin-sitemap": "^2.2.19",
    "gatsby-plugin-styled-components": "^3.1.11",
    "gatsby-plugin-typography": "^2.3.14",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-sharp": "^2.3.5",
    "prop-types": "^15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "styled-components": "^4.4.0",
    "styled-system": "^5.1.2",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.17",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.17.0",
    "pretty-quick": "^2.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
